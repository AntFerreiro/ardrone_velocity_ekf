#!/usr/bin/env python
PACKAGE = "velocity_control"

from dynamic_reconfigure.parameter_generator_catkin import *

gen = ParameterGenerator()

gen.add("Kp",     double_t,    0, "Proportional Coefficient in X direction", 0.45,  0.0, 2.0)
gen.add("Ki",     double_t,    0, "Integral Coefficient in X direction", 0.15,  0.0, 1.0)
gen.add("Kd",     double_t,    0, "Derivative Coefficient in X direction", 0.05,  0.0, 1.0)
gen.add("limit_x",     double_t,    0, "Maximum control output in x direction", 0.5,  0.0, 1.0)
gen.add("limit_y",     double_t,    0, "Maximum control output in y direction", 0.5,  0.0, 1.0)
gen.add("windup",     double_t,    0, "Windup  Coefficient of Integral Term", 0.6,  0.0, 1.0)
gen.add("beta",     double_t,    0, "Set point weighting", 0.9,  0.0, 1.0)

exit(gen.generate(PACKAGE, "velocity_control", "dynamic_param_config"))
